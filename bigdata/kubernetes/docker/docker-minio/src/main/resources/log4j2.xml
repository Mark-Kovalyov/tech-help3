<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="ALL">

    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5level] %t %logger - %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>

</Configuration>